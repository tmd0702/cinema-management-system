<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-pref-height: 600; -fx-pref-width: 900;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GraphicalUserInterface.ManagementViewController">
   <children>
      <VBox fx:id="accountManagementView" layoutX="110.0" prefHeight="600.0" prefWidth="790.0">
         <children>
            <HBox fx:id="menuBox" prefHeight="46.0" prefWidth="790.0" />
            <ScrollPane fx:id="dataViewScrollPane" prefHeight="527.0" prefWidth="790.0" />
            <HBox prefHeight="29.0" prefWidth="790.0">
               <children>
                  <Button fx:id="backToHeadBtn" mnemonicParsing="false" prefHeight="52.0" prefWidth="52.0" text="&lt;&lt;" />
                  <Button fx:id="backBtn" mnemonicParsing="false" prefHeight="52.0" prefWidth="52.0" text="&lt;" />
                  <Label prefHeight="52.0" prefWidth="52.0" style="-fx-alignment: CENTER;" text="Page" />
                  <TextField fx:id="pageInputField" prefHeight="52.0" prefWidth="52.0" />
                  <Label fx:id="totalPageNumLabel" prefHeight="52.0" prefWidth="52.0" style="-fx-alignment: CENTER;" text="of" />
                  <Button fx:id="nextBtn" mnemonicParsing="false" prefHeight="52.0" prefWidth="52.0" text="&gt;" />
                  <Button fx:id="nextToTailBtn" mnemonicParsing="false" prefHeight="52.0" prefWidth="52.0" text="&gt;&gt;" />
                  <ImageView fx:id="pagingToolbarRefreshBtn" fitHeight="29.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true" />
                  <TextField fx:id="numRowPerPageInputField" prefHeight="52.0" prefWidth="52.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox fx:id="tabPane" prefHeight="225.0" prefWidth="143.0" style="-fx-pref-width: 110; -fx-pref-height: 600; -fx-background-color: #333333;">
         <children>
            <ImageView fx:id="logoImageView" fitHeight="76.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </ImageView>
         </children>
      </VBox>
   </children>
</AnchorPane>
